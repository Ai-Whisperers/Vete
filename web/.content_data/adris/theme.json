{
  "colors": {
    "primary": {
      "main": "#2F5233",
      "light": "#4E7C54",
      "dark": "#1A331D",
      "contrast": "#FFFFFF"
    },
    "secondary": {
      "main": "#F0C808",
      "light": "#F5D74D",
      "dark": "#B39506",
      "contrast": "#1A331D"
    },
    "accent": "#F0C808",
    "background": {
      "default": "#FAFBFC",
      "paper": "#FFFFFF",
      "subtle": "#F2F4F6",
      "dark": "#1A331D",
      "hero": "#2F5233"
    },
    "text": {
      "primary": "#1F2937",
      "secondary": "#4B5563",
      "muted": "#9CA3AF",
      "invert": "#FFFFFF",
      "link": "#2F5233"
    },
    "status": {
      "success": "#10B981",
      "warning": "#F59E0B",
      "error": "#EF4444",
      "info": "#3B82F6"
    },
    "border": {
      "light": "#E5E7EB",
      "default": "#D1D5DB",
      "dark": "#9CA3AF"
    }
  },
  "gradients": {
    "hero": "linear-gradient(135deg, #2F5233 0%, #1A331D 100%)",
    "primary": "linear-gradient(135deg, #4E7C54 0%, #2F5233 100%)",
    "accent": "linear-gradient(135deg, #F0C808 0%, #F5D74D 100%)",
    "dark": "linear-gradient(180deg, #1A331D 0%, #0F1F11 100%)",
    "card": "linear-gradient(145deg, #FFFFFF 0%, #F8FAFC 100%)"
  },
  "fonts": {
    "heading": "'Montserrat', 'Segoe UI', sans-serif",
    "body": "'Inter', 'Segoe UI', sans-serif",
    "mono": "'JetBrains Mono', 'Fira Code', monospace"
  },
  "typography": {
    "h1": {
      "size": "3.5rem",
      "weight": "900",
      "line_height": "1.1"
    },
    "h2": {
      "size": "2.5rem",
      "weight": "800",
      "line_height": "1.2"
    },
    "h3": {
      "size": "1.75rem",
      "weight": "700",
      "line_height": "1.3"
    },
    "body": {
      "size": "1rem",
      "weight": "400",
      "line_height": "1.6"
    },
    "small": {
      "size": "0.875rem",
      "weight": "400",
      "line_height": "1.5"
    }
  },
  "ui": {
    "border_radius": "16px",
    "border_radius_sm": "8px",
    "border_radius_lg": "24px",
    "border_radius_full": "9999px",
    "shadow_sm": "0 1px 2px 0 rgb(0 0 0 / 0.05)",
    "shadow_md": "0 4px 6px -1px rgb(0 0 0 / 0.1), 0 2px 4px -2px rgb(0 0 0 / 0.1)",
    "shadow_lg": "0 10px 15px -3px rgb(0 0 0 / 0.1), 0 4px 6px -4px rgb(0 0 0 / 0.1)",
    "shadow_xl": "0 20px 25px -5px rgb(0 0 0 / 0.1), 0 8px 10px -6px rgb(0 0 0 / 0.1)",
    "shadow_card": "0 4px 20px -2px rgb(47 82 51 / 0.12)",
    "shadow_button": "0 4px 14px 0 rgb(47 82 51 / 0.25)"
  },
  "spacing": {
    "section_padding": "6rem",
    "container_max_width": "1280px",
    "card_padding": "2rem",
    "input_padding": "0.75rem 1rem"
  },
  "animations": {
    "transition_fast": "150ms ease",
    "transition_normal": "300ms ease",
    "transition_slow": "500ms ease",
    "hover_scale": "1.02",
    "hover_lift": "-4px"
  }
}
