{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings 0"
    ],
    "*.{json,md,css}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "scripts": {
    "prepare": "husky",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "test": "npm run test:unit && npm run test:integration && npm run test:api && npm run test:e2e",
    "test:all": "npm run test",
    "test:watch": "vitest watch",
    "test:unit": "vitest run tests/unit --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts --coverage",
    "test:system": "vitest run tests/system --coverage",
    "test:functionality": "vitest run tests/functionality --coverage",
    "test:uat": "vitest run tests/uat --coverage",
    "test:api": "vitest run tests/api --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:critical": "vitest run --testPathPattern='critical' && playwright test --grep '@critical'",
    "test:smoke": "vitest run tests/unit && playwright test e2e/public",
    "test:feature:pets": "vitest run --testPathPattern='pets' && playwright test --grep 'pets'",
    "test:feature:booking": "vitest run --testPathPattern='booking|appointment'",
    "test:feature:vaccines": "vitest run --testPathPattern='vaccine'",
    "test:feature:inventory": "vitest run --testPathPattern='inventory'",
    "test:feature:finance": "vitest run --testPathPattern='finance|expense'",
    "test:feature:medical": "vitest run --testPathPattern='medical-records|prescription'",
    "test:feature:store": "vitest run --testPathPattern='store|cart|product' && playwright test e2e/store",
    "test:security": "vitest run tests/security --coverage",
    "test:public": "playwright test e2e/public",
    "test:store": "playwright test e2e/store",
    "test:portal": "playwright test e2e/portal",
    "test:dashboard": "playwright test e2e/dashboard",
    "test:tools": "playwright test e2e/tools",
    "test:coverage": "vitest run --coverage",
    "test:coverage:html": "vitest run --coverage --coverage.reporter=html",
    "test:db:reset": "node scripts/reset-test-db.js",
    "test:db:seed": "node scripts/seed-test-db.js",
    "test:db:setup": "npm run test:db:reset && npm run test:db:seed",
    "sync:images": "npx tsx scripts/sync-clinic-images.ts",
    "sync:images:watch": "npx tsx scripts/sync-clinic-images.ts --watch",
    "inventory:template": "npx tsx scripts/generate-inventory-template.ts",
    "inventory:gsheet:create": "npx tsx scripts/sync-inventory-template-gsheet.ts --create",
    "inventory:gsheet:update": "npx tsx scripts/sync-inventory-template-gsheet.ts --update",
    "db:setup": "node db/setup-db.mjs full",
    "db:schema": "node db/setup-db.mjs schema",
    "db:clean": "node db/setup-db.mjs clean",
    "seed": "npx tsx db/seeds/scripts/seed.ts",
    "seed:basic": "npx tsx db/seeds/scripts/seed.ts --type basic",
    "seed:reference": "npx tsx db/seeds/scripts/seed.ts --type reference",
    "seed:full": "npx tsx db/seeds/scripts/seed.ts --type full",
    "seed:demo": "npx tsx db/seeds/scripts/seed.ts --type demo",
    "seed:clear": "npx tsx db/seeds/scripts/seed.ts --clear --type demo",
    "seed:reset": "npx tsx db/seeds/scripts/seed.ts --clear --type demo",
    "seed:dashboard": "npx tsx scripts/seed-dashboard-test-data.ts",
    "seed:v2": "npx tsx db/seeds/scripts/seed-v2.ts",
    "seed:v2:basic": "npx tsx db/seeds/scripts/seed-v2.ts --variant basic",
    "seed:v2:integration": "npx tsx db/seeds/scripts/seed-v2.ts --variant integration",
    "seed:v2:e2e": "npx tsx db/seeds/scripts/seed-v2.ts --variant e2e",
    "seed:v2:demo": "npx tsx db/seeds/scripts/seed-v2.ts --variant demo",
    "seed:v2:reset": "npx tsx db/seeds/scripts/seed-v2.ts --variant reset",
    "seed:v2:dry-run": "npx tsx db/seeds/scripts/seed-v2.ts --variant demo --dry-run",
    "seed:v2:verbose": "npx tsx db/seeds/scripts/seed-v2.ts --variant demo --verbose",
    "seed:v2:adris": "npx tsx db/seeds/scripts/seed-v2.ts --variant demo --tenant adris",
    "seed:v2:petlife": "npx tsx db/seeds/scripts/seed-v2.ts --variant demo --tenant petlife",
    "seed:v2:all": "npx tsx db/seeds/scripts/seed-v2.ts --variant demo --tenants adris,petlife",
    "seed:demo-data": "npx tsx db/seeds/scripts/seed-demo-data.ts",
    "seed:demo-data:all": "npx tsx db/seeds/scripts/seed-demo-data.ts --tenants adris,petlife --verbose",
    "db:fixes": "node db/setup-db.mjs fixes",
    "reset-dev": "powershell -ExecutionPolicy Bypass -File reset-dev.ps1",
    "kill-node": "powershell -ExecutionPolicy Bypass -File kill-node.ps1 -Force"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@react-pdf/renderer": "^4.3.1",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.88.0",
    "@tanstack/react-query": "^5.90.12",
    "@types/pg": "^8.16.0",
    "@types/qrcode": "^1.5.6",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.35.8",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "autoprefixer": "^10.4.23",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "framer-motion": "^12.23.26",
    "html5-qrcode": "^2.3.8",
    "inngest": "^3.48.1",
    "lodash.merge": "^4.6.2",
    "lucide-react": "^0.561.0",
    "next": "15.5.9",
    "next-intl": "^4.7.0",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "postgres": "^3.4.7",
    "qrcode": "^1.5.4",
    "react": "^19.2.3",
    "react-big-calendar": "^1.19.4",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.69.0",
    "react-is": "^19.2.3",
    "recharts": "^3.6.0",
    "resend": "^6.6.0",
    "swagger-ui-react": "^5.31.0",
    "tailwind-merge": "^3.4.0",
    "twilio": "^5.10.7",
    "use-debounce": "^10.0.6",
    "xlsx": "^0.18.5",
    "zod": "^4.2.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@mswjs/data": "^0.16.2",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/nextjs": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^30.0.0",
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-big-calendar": "^1.16.3",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "15.5.9",
    "googleapis": "^169.0.0",
    "husky": "^9.1.7",
    "jsdom": "^27.3.0",
    "lint-staged": "^16.2.7",
    "msw": "^2.12.4",
    "nyc": "^17.1.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "storybook": "^8.6.14",
    "supertest": "^7.1.4",
    "swagger-jsdoc": "^6.2.8",
    "tailwindcss": "^3.4.19",
    "typescript": "^5",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
}
